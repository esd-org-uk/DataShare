<?xml version="1.0" encoding="UTF-8"?>

<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi" xmlns:netfx="http://schemas.microsoft.com/wix/NetFxExtension"
     xmlns:util='http://schemas.microsoft.com/wix/UtilExtension'
     xmlns:iis="http://schemas.microsoft.com/wix/IIsExtension"
     xmlns:sql='http://schemas.microsoft.com/wix/SqlExtension'>
  <!-- definitions START-->
  <?define SkuName = "DataShare"?>
  <?define ProductName = "DataShare"?>

  
  <?define MinProductVersion="1.3.0" ?>
  <?define UpgradeCode="{f8271709-2ed5-48b8-8344-a569c799d619}"?>
  <?define Manufacturer="Redbridge Council"?>
  <!-- To change the installer to be a new version 
  #step 1 
  change the RTMProductVersion to a higher number 
  #step 2 
  generate new package code and replace existing package code guid
  
  #step 3 
  generate new product code and replace existing product code
  see below: 
  -->
  <?define RTMProductVersion="1.5.0" ?>
  <?define PackageCode="{082cbbad-2eea-4728-827c-fccdc44b43ac}"?>
  <?define ProductCode="{c4dde142-844d-409d-b532-c4df3ba76001}"?>
  
  
  <!-- definitions END-->

  <Product Id="$(var.ProductCode)"
           Name="$(var.ProductName)"
           Language="1033"
           Version="$(var.RTMProductVersion)"
           Manufacturer="$(var.Manufacturer)"
           UpgradeCode="$(var.UpgradeCode)">
    <Package Id="$(var.PackageCode)" InstallerVersion="200" Compressed="yes" InstallScope="perMachine" InstallPrivileges="elevated"/>

    <MediaTemplate EmbedCab="yes" />
    <WixVariable Id="WixUIBannerBmp" Value="bitmaps\topbanner.bmp" />
    <WixVariable Id="WixUIDialogBmp" Value="bitmaps\banner.bmp" />


    <Upgrade Id="$(var.UpgradeCode)">
      <UpgradeVersion Minimum="$(var.MinProductVersion)"
                      IncludeMinimum="no"
                      OnlyDetect="yes"
                      Language="1033"
                      Property="NEWPRODUCTFOUND" />
      <UpgradeVersion Minimum="$(var.MinProductVersion)"
                      IncludeMinimum="yes"
                      Maximum="$(var.RTMProductVersion)"
                      IncludeMaximum="no"
                      Language="1033"
                      Property="UPGRADECODE"
                      />
    </Upgrade>
    <Property Id="ARPNOREPAIR" Value="yes" Secure="yes" />

    <Property Id='WEBINSTALLATIONDIR'>
      <RegistrySearch Id='FindInstallLocation2' Root='HKLM'
                      Key='Software\Microsoft\Windows\CurrentVersion\Uninstall\[UPGRADECODE]'
                      Name='InstallLocation2' Type='directory' />
    </Property>
    <Property Id="UPGRADEVERSION" Value="$(var.RTMProductVersion)"></Property>
    <Property Id="UPGRADECODE"></Property>
    <Property Id="WEBSITENAME" Value="DataShareWeb" />
    <Property Id="WEBSITEAPPPOOL" Value="DataShareAppPool" />

    <Property Id="INSTALL_IIS_SITE" Value="True"></Property>
    <Property Id='WEBINSTALLATIONDIR_FOR_COPY'>
      <RegistrySearch Id='FindInstallLocation' Root='HKLM'
                      Key='Software\Microsoft\Windows\CurrentVersion\Uninstall\$(var.UpgradeCode)'
                      Name='InstallLocation' Type='directory' />
    </Property>
    <!--start initial values for installers-->
    <Property Id="DATABASE_NAME" Value="DataShare" />
    <Property Id="DATABASE_RUNTIME_USERNAME" Value="DataShare" />
    <Property Id="DATABASE_RUNTIME_PASSWORD" />

    <Property Id="DATABASE_RUNTIME_PASSWORD" Value="PutPasswordHere">
      <RegistrySearch Id='CheckIfUpgrade' Root='HKLM'
                Key='Software\Microsoft\Windows\CurrentVersion\Uninstall\$(var.UpgradeCode)'
                Name='InstallLocation' Type='directory' />
    </Property>

    <Property Id="DATASHARE_URL" Value="data.mycouncil.gov.uk" Secure="yes"/> <!--preloaded values  - user can change on installation dialogs-->
    <Property Id="COUNCIL_URL" Value="http://" />
    <Property Id="COUNCIL_URI" Value="http://opendatacommunities.org/id/london-borough-council/mycouncil" />
    <!--end initial values for installers-->
    <Property Id="RUNTIME_DATABASE_LOGON_TYPE" Value="WinAuth" />
    <Property Id="CREATE_DATABASE_LOGON_TYPE" Value="SqlAuth" />
    <!-- Check to see if IIS is installed. It it's not, error out. -->
    <Property Id="IIS_MAJOR_VERSION">
      <RegistrySearch Id="CheckIISVersion"
                      Root="HKLM"
                      Key="SOFTWARE\Microsoft\InetStp"
                      Name="MajorVersion"
                      Type="raw" />
    </Property>
    <Condition Message="IIS must be installed">
      Installed OR IIS_MAJOR_VERSION
    </Condition>
    <Property Id="ISSQLSERVERSERVICEINSTALLED">
      <RegistrySearch Id="IsSqlServerServiceInstalled" Root="HKLM" Key="SYSTEM\CurrentControlSet\services\MSSQLSERVER"
                      Name="Description" Type="raw" />
    </Property>

    <!-- Go find the IIS root directory from the registry. On most machines 
				 that defaults to C:\inetpub\wwwroot. This will be the directory we 
				 install into. -->
    <Property Id="IISROOT">
      <RegistrySearch Id="IISROOT"
                      Type="directory"
                      Root="HKLM"
                      Key="Software\Microsoft\InetStp"
                      Name="PathWWWRoot" />
    </Property>

    <Condition Message="IIS does not appear to be installed correctly, the  
											 root directory is not set.">
      Installed OR IISROOT
    </Condition>
    <Property Id='ASP_NET_MVC_3_0_DLL_64BIT'>
      <RegistrySearch Id='regsearch' Root="HKLM"
                      Key="SOFTWARE\Wow6432Node\Microsoft\.NETFramework\v4.0.30319\AssemblyFoldersEx\ASP.NET MVC 3"
                      Type="raw" />
    </Property>
    <Property Id='ASP_NET_MVC_3_0_DLL_32BIT'>
      <RegistrySearch Id='regsearch32bit' Root="HKLM"
                      Key="SOFTWARE\Microsoft\.NETFramework\v4.0.30319\AssemblyFoldersEx\ASP.NET MVC 3" Type="raw" />
    </Property>
    <Condition
      Message='ASP.NET MVC 3.0 is required to proceed with the installation. It can be installed via the Web Platform Installer.'>
      Installed OR ASP_NET_MVC_3_0_DLL_64BIT OR ASP_NET_MVC_3_0_DLL_32BIT
    </Condition>

    <!--end conditional checking-->


    <Binary Id='CreateUsers' SourceFile='SqlScripts\CreateUsers.sql' />
    <Binary Id='CreateTables' SourceFile='SqlScripts\CreateTables.sql' />
    <Binary Id='InsertData' SourceFile='SqlScripts\InsertData.sql' />
    <Binary Id='Combined_1_4_0_script' SourceFile='SqlScripts\Combined_1_4_0_script.sql' />
    <Binary Id='Combined_1_5_0_script' SourceFile='SqlScripts\Combined_1_5_0_script.sql' />

    <Property Id="QtExecCmdLine" Value='&quot;cmd.exe&quot; /Q /C For %X in (&quot;[WEBINSTALLATIONDIR]web.config&quot; &quot;[WEBINSTALLATIONDIR]Content\images\CouncilLogo.png&quot; ) do copy /y %X [DATASHAREFOLDER]' />
    



    <InstallExecuteSequence>
      <RemoveExistingProducts After="InstallInitialize" />
      <!--<Custom Action="cmd_rename_web_config_bak_to_web_config" After="CostFinalize">((NOT UPGRADECODE ="") AND (NOT REMOVE="ALL")) </Custom>
      <Custom Action="prop_rename_web_config_bak_to_web_config" After="cmd_copy_webconfig_backup_to_webroot">((NOT UPGRADECODE ="") AND (NOT REMOVE="ALL"))</Custom>-->

      

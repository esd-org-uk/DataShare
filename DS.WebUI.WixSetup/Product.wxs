    <CustomAction Id="cmd_rename_web_config_bak_to_web_config" Property="prop_rename_web_config_bak_to_web_config"
                  Value="&quot;cmd.exe&quot; /Q /C copy /Y [WEBINSTALLATIONDIR]web.config.bak [WEBINSTALLATIONDIR]web.config"
                  Execute="immediate" />

    <CustomAction Id="prop_rename_web_config_bak_to_web_config" BinaryKey="WixCA" DllEntry="CAQuietExec"
                  Execute="deferred" Return="check" Impersonate="no" />

    <CustomAction Id="cmd_create_web_config_backup_when_upgrading"

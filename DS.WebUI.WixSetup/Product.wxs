      <Custom Action="prop_create_web_config_backup_when_upgrading" Before="InstallFinalize">NOT Installed AND (NOT REMOVE="ALL") AND (NOT WEBINSTALLATIONDIR_FOR_COPY = "") AND (UPGRADECODE="")</Custom>

      <Custom Action="cmd_create_web_config_backup" After="CostFinalize">NOT Installed AND (NOT REMOVE="ALL") AND (NOT WEBINSTALLATIONDIR = "") AND (UPGRADECODE="")</Custom>
      <Custom Action="prop_create_web_config_backup" Before="InstallFinalize">NOT Installed AND (NOT REMOVE="ALL") AND (NOT WEBINSTALLATIONDIR = "") AND (UPGRADECODE="")</Custom>

      <Custom Action="cmd_create_logo_backup" After="CostFinalize">NOT Installed AND (NOT REMOVE="ALL") AND (NOT WEBINSTALLATIONDIR = "") AND (UPGRADECODE="")</Custom>
      <Custom Action="prop_create_logo_backup" Before="InstallFiles">NOT Installed AND (NOT REMOVE="ALL") AND (NOT WEBINSTALLATIONDIR = "") AND (UPGRADECODE="")</Custom>

      <Custom Action="cmd_create_oldwebconfig_backup" After="CostFinalize">NOT Installed AND (NOT REMOVE="ALL") AND (NOT WEBINSTALLATIONDIR = "") AND (UPGRADECODE="")</Custom>
      <Custom Action="prop_create_oldwebconfig_backup" Before="InstallFiles">NOT Installed AND (NOT REMOVE="ALL") AND (NOT WEBINSTALLATIONDIR = "") AND (UPGRADECODE="")</Custom>


      <Custom Action="cmd_create_logo_backup_upgrade" After="CostFinalize">NOT Installed AND (NOT REMOVE="ALL") AND (NOT WEBINSTALLATIONDIR = "") AND (UPGRADECODE="")</Custom>
      <Custom Action="prop_create_logo_backup_upgrade" Before="InstallFiles">NOT Installed AND (NOT REMOVE="ALL") AND (NOT WEBINSTALLATIONDIR = "") AND (UPGRADECODE="")</Custom>

      <Custom Action="cmd_rename_logo_bak_to_councillogo" After="InstallFiles">NOT Installed AND (NOT REMOVE="ALL") AND (NOT WEBINSTALLATIONDIR = "") AND (UPGRADECODE="")</Custom>
      <Custom Action="prop_rename_logo_bak_to_councillogo" Before="InstallFinalize">NOT Installed AND (NOT REMOVE="ALL") AND (NOT WEBINSTALLATIONDIR = "") AND (UPGRADECODE="")</Custom>

      <Custom Action="QtExecExample" Before="InstallInitialize">
        NOT Installed AND (NOT REMOVE="ALL") AND (NOT WEBINSTALLATIONDIR = "") AND (NOT UPGRADECODE="")
      </Custom>
      
      <Custom Action="cmd_copy_webconfig_backup_to_webroot" After="CostFinalize">NOT Installed AND (NOT REMOVE="ALL") AND (NOT WEBINSTALLATIONDIR = "") AND (NOT UPGRADECODE="")</Custom>
      <Custom Action="prop_copy_webconfig_backup_to_webroot" Before="InstallFinalize">NOT Installed AND (NOT REMOVE="ALL") AND (NOT WEBINSTALLATIONDIR = "") AND (NOT UPGRADECODE="")</Custom>

      <Custom Action="cmd_copy_logo_backup_to_images" After="CostFinalize">NOT Installed AND (NOT REMOVE="ALL") AND (NOT WEBINSTALLATIONDIR = "") AND (NOT UPGRADECODE="")</Custom>
      <Custom Action="prop_copy_logo_backup_to_images" Before="InstallFinalize">NOT Installed AND (NOT REMOVE="ALL") AND (NOT WEBINSTALLATIONDIR = "") AND (NOT UPGRADECODE="")</Custom>

    </InstallExecuteSequence>
    

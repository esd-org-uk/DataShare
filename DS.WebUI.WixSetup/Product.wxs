      <Component Id="SqlComponent" Guid="4885828f-9f2d-49b1-9693-9f41166fd5c0" KeyPath="yes">
        <Condition>NOT CREATE_DATABASE_LOGON_TYPE = "SqlAuth" AND UPGRADECODE = ""</Condition>
        <sql:SqlDatabase Id="SqlDatabase" Database="[DATABASE_NAME]" Server="[DATABASE_SERVER]"
                         CreateOnInstall="yes" DropOnUninstall="no" ContinueOnError="no">

          <sql:SqlString Id="SetDBOnline" SQL="ALTER DATABASE [DATABASE_NAME] SET ONLINE " ExecuteOnInstall="yes"
                         Sequence="1">
          </sql:SqlString>
          <sql:SqlString Id="CreateDBRunTimeUser"
                         SQL="                       
IF (1 = FULLTEXTSERVICEPROPERTY('IsFullTextInstalled'))
begin
	EXEC [DATABASE_NAME].dbo.sp_fulltext_database @action = 'enable'
end
IF NOT EXISTS (SELECT * FROM sys.database_principals WHERE name = N'[DATABASE_RUNTIME_USERNAME]')
CREATE USER [DATABASE_RUNTIME_USERNAME] WITHOUT LOGIN WITH DEFAULT_SCHEMA=dbo

If not Exists (select loginname from master.dbo.syslogins where name = '[DATABASE_RUNTIME_USERNAME]')
Begin
	CREATE LOGIN [DATABASE_RUNTIME_USERNAME] with PASSWORD = '[DATABASE_RUNTIME_PASSWORD]' ,  DEFAULT_DATABASE = [DATABASE_NAME];
  alter authorization on database::[DATABASE_NAME] to [DATABASE_RUNTIME_USERNAME];
END
	Exec sp_addrolemember N'db_owner', N'[DATABASE_RUNTIME_USERNAME]';
	

                         "
                         ExecuteOnInstall="yes" Sequence="2">
          </sql:SqlString>
          <sql:SqlScript Id='CreateUsers' BinaryKey='CreateUsers' ExecuteOnInstall='yes' Sequence='3' />
          <sql:SqlScript Id='CreateTables' BinaryKey='CreateTables' ExecuteOnInstall='yes' Sequence='4' />
          <sql:SqlScript Id='InsertData' BinaryKey='InsertData' ExecuteOnInstall='yes' Sequence='5' />
          <sql:SqlString Id="InsertSystemConfiguration"
                         SQL="                       
				IF NOT EXISTs(SELECT * FROM [DATABASE_NAME].dbo.SYS_DS_ConfigurationSettings WHERE SettingId = 1)
				INSERT INTO [DATABASE_NAME].dbo.SYS_DS_ConfigurationSettings
				(SettingId
				,CouncilName
				,CouncilUrl
				,CouncilUri
				,MapCentreLatitude
				,MapCentreLongitude
				,MapDefaultZoom
				,AnalyticsTrackingRef
				)
				VALUES
				(1,'','','','52.5897007687178','-0.98876953125','6','')
												"
                         ExecuteOnInstall="yes" Sequence="6">

          </sql:SqlString>
          <sql:SqlScript Id='Combined_1_4_0_script' BinaryKey='Combined_1_4_0_script' ExecuteOnInstall='yes'
